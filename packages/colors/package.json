{
  "name": "@carbon/colors",
  "description": "Colors for digital and software products using the Carbon Design System",
  "version": "0.0.1-alpha.25",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/IBM/carbon-elements/tree/master/packages/colors",
  "bugs": "https://github.com/IBM/carbon-elements/issues",
  "files": [
    "es",
    "lib",
    "scss",
    "umd"
  ],
  "keywords": [
    "eyeglass-module",
    "ibm",
    "elements",
    "carbon",
    "carbon-elements",
    "carbon-design-system"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile && node tasks/generateSass.js",
    "clean": "rimraf es lib umd scss",
    "compile": "yarn compile:es && yarn compile:cjs && yarn compile:umd",
    "compile:es": "tsc --lib es2017 --module es2015 --target es5 --outDir es/",
    "compile:cjs": "tsc --lib es2017 --module commonjs --target es5 --outDir lib/",
    "compile:umd": "tsc --lib es2017 --module umd --target es5 --outDir umd/"
  },
  "devDependencies": {
    "@carbon/bundler": "0.0.1-alpha.25",
    "@carbon/cli-reporter": "0.0.1-alpha.25",
    "fs-extra": "^7.0.0",
    "node-sass": "^4.9.4",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "typescript": "^3.2.2"
  },
  "eyeglass": {
    "exports": false,
    "name": "@carbon/colors",
    "sassDir": "scss",
    "needs": "^1.3.0"
  }
}
